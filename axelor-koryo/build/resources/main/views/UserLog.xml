<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
			  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.2.xsd">

		<grid model="com.axelor.apps.koryo.db.UserLog" title="User log" name="user-log-grid">
			<field name="logDate"/>
			<field name="logUser"/>
			<field name="message"/>	
		</grid>

		<form model="com.axelor.apps.koryo.db.UserLog" title="User log" name="user-log-form">
		 <panel name="mainPanel">
			<field name="logDate"/>
			<field name="logUser"/>
			<field name="message" widget="html" colSpan="12"/>
		 </panel>	
		</form>
			  
</object-views>			  